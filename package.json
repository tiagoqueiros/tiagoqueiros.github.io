{
  "name": "tiagoqueiros.github.io",
  "version": "1.0.0",
  "description": "My personal website made with Nuxt.js and Markdown",
  "author": "Tiago QueirÃ³s",
  "private": true,
  "scripts": {
    "nuxt": "rm -rf dist && rm -rf .nuxt && nuxt",
    "ssr": "yarn ssr:build && nuxt start",
    "ssr:build": "yarn nuxt build",
    "ssr:analyze": "yarn ssr:build --analyze",
    "spa": "yarn nuxt build --spa",
    "spa:ci": "yarn spa --fail-on-error",
    "spa:analyze": "yarn spa --analyze",
    "static": "yarn nuxt generate",
    "static:ci": "yarn static --fail-on-error",
    "lint:js": "eslint --quiet '**/*.*'",
    "lint:style": "stylelint '**/*.*'",
    "lint:all": "yarn lint:js && yarn lint:style",
    "ftp-deploy": "nodemon ci/deploy.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.4",
    "lodash": "^4.17.20",
    "nuxt": "^2.14.12",
    "vue": "^2.6.11",
    "vue-animejs": "^2.1.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/markdownit": "^1.2.10",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.1.11",
    "babel-eslint": "^10.0.1",
    "breakpoint-sass": "^2.7.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-nuxt": ">=2.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "typescript": "^4.1.3",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.2"
  }
}
