{
  "name": "tiagoqueiros.github.io",
  "version": "1.0.0",
  "description": "My personal website made with Nuxt.js and Markdown.",
  "author": "Tiago Queir√≥s",
  "private": true,
  "scripts": {
    "nuxt": "rm -rf dist && rm -rf .nuxt && nuxt",
    "ssr": "yarn ssr:build && nuxt start",
    "ssr:build": "yarn nuxt build",
    "ssr:analyze": "yarn ssr:build --analyze",
    "spa": "yarn nuxt build --spa",
    "spa:ci": "yarn spa --fail-on-error",
    "spa:analyze": "yarn spa --analyze",
    "static": "yarn nuxt generate",
    "static:ci": "yarn static --fail-on-error",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx,vue --quiet .",
    "lint:stylelint": "stylelint ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "highlight.js": "^11.0.1",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.7",
    "vue": "^2.6.11",
    "vue-animejs": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/markdownit": "^2.0.0",
    "@nuxtjs/style-resources": "^1.1.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.1.11",
    "@pixelmatters/eslint-config-pixelmatters": "^1.0.2",
    "@pixelmatters/prettier-config-pixelmatters": "^1.0.0",
    "@pixelmatters/stylelint-config-pixelmatters": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "breakpoint-sass": "^2.7.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.3.1",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.4",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.46.0"
  },
  "resolutions": {
    "highlight.js": "^10.4.1",
    "is-svg": "^4.2.2"
  }
